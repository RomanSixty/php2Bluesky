{
    "name": "williamsdb/php2bluesky",
    "description": "A library that makes posting to Bluesky from PHP simple.",
    "keywords": ["Bluesky", "AT Protocol", "PHP"],
    "homepage": "https://php2Bluesky.dev",
    "license": "GPL-3.0-or-later",
    "authors": [
      {
        "name": "Neil Thompson",
        "homepage": "https://spokenlikeageek.com",
        "role": "Developer"
      }
    ],
    "support": {
      "issues": "https://github.com/williamsdb/php2Bluesky/issues",
      "source": "https://github.com/williamsdb/php2Bluesky"
    },
      "type": "library",
    "autoload": {
        "psr-4": {
            "williamsdb\\php2bluesky\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": "^8.0",
        "cjrasmussen/bluesky-api": "^2.0",
        "ext-dom": "*",
        "ext-gd": "*"
    }
}
